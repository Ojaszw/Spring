<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:c="http://www.springframework.org/schema/c"
	xmlns:p="http://www.springframework.org/schema/p"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean class="com.capg.bean.EmployeeList" id="list">
	
	<property name="empList">
	<list>
	<ref bean="emp1"/>
	<ref bean="emp2"/>
	</list></property>
	</bean>
	<bean class="com.capg.bean.Employee" id="emp1">
	<property name="empId" value="001"/>
	<property name="empName" value="Raman"/>
	<property name="salary" value="870000"/>
	<property name="businUnit" value="Production"/>
	<property name="age" value="34"/>
	</bean>
	<bean class="com.capg.bean.Employee" id="emp2">
	<property name="empId" value="002"/>
	<property name="empName" value="Rajiv"/>
	<property name="salary" value="989999"/>
	<property name="businUnit" value="B-SE"/>
	<property name="age" value="44"/>
	</bean>
	
</beans>
